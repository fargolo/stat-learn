# update em w
delta_w <- eta * (y[i] - ypred) * as.numeric(x[i, ])
#nota: x[i,] sera multiplicado como matriz (dot product)
w <- w + delta_w
ypreds[i] <- ypred # salva predicao atual
}
print(paste("Weights: ",w))
return(ypreds)
}
