normalize <- function(x){
x.mean <- mean(x)
x.std <- sd(x)
y <- vapply(X=x,
FUN=function(x) 1/x.std*(sqrt(2*pi)*exp((-0.5)*(x-x.mean/x.std)^2)),
FUN.VALUE=1)
return(y)
}
